set terminal png size 800,600
set key ins vert
set key right top
set output "MemoryUse.png"
set xrange [0:150.000]
set yrange [0:9508.846]
set xlabel "Memory usage"
set ylabel "Megaflops per Sec"
 
plot \
"blk.dat" using 1:2 with linespoints, \
"lib.dat" using 1:2 with linespoints, \
"nat_kmn.dat" using 1:2 with linespoints, \
"nat_knm.dat" using 1:2 with linespoints, \
"nat_mkn.dat" using 1:2 with linespoints, \
"nat_mnk.dat" using 1:2 with linespoints, \
"nat_nkm.dat" using 1:2 with linespoints, \
"nat_nmk.dat" using 1:2 with linespoints
set output "checker.png"
set xrange [0:150.000]
set yrange [0:9.98704e+07]
set xlabel "Memory usage"
set ylabel "Max Error   "
plot \
"blk.dat" using 1:3 with linespoints, \
"lib.dat" using 1:3 with linespoints, \
"nat_kmn.dat" using 1:3 with linespoints, \
"nat_knm.dat" using 1:3 with linespoints, \
"nat_mkn.dat" using 1:3 with linespoints, \
"nat_mnk.dat" using 1:3 with linespoints, \
"nat_nkm.dat" using 1:3 with linespoints, \
"nat_nmk.dat" using 1:3 with linespoints
